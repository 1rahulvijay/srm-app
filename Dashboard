<script src="https://your-tableau-server/javascripts/api/tableau.embedding.min.js"></script>
<div id="tableauViz"></div>

<script>
  const vizDiv = document.getElementById("tableauViz");
  const vizUrl = "{{ tableau_url }}";

  const options = {
    hideTabs: true,
    onFirstInteractive: function () {
      console.log("Viz loaded");
    }
  };

  const viz = new tableau.Viz(vizDiv, vizUrl, options);
</script>
